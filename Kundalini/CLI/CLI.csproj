    <TargetFramework>netcoreapp2.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\ChakraCoreHost\ChakraCoreHost.csproj" />
    <ProjectReference Include="..\Kundalini\Kundalini.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Scripts.Designer.cs">
